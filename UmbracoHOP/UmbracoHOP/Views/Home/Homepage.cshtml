        <h2 >Total Countries</h2>
        <hr>
        <br/>
        <br/>
        <h1>@ViewBag.TotalCountries</h1>
    </div>
    <div class="box">
        <h2>Total Cities</h2>
        <hr>
        <br/>
        <br/>
        <h1>@ViewBag.TotalCities</h1>
    </div>
</div>

